<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mapping.filmed7x7" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>filmed7x7</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="ios">
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="www/img/icon/ios7/Icon-60.png" width="60" height="60" />
        <icon src="www/img/icon/ios7/Icon-60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="www/img/icon/ios7/Icon-76.png" width="76" height="76" />
        <icon src="www/img/icon/ios7/Icon-76@2x.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="www/img/icon/ios7/Icon-40.png" width="40" height="40" />
        <icon src="www/img/icon/ios7/Icon-40@2x.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="www/img/icon/ios7/Icon.png" width="57" height="57" />
        <icon src="www/img/icon/ios7/Icon@2x.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="www/img/icon/ios7/Icon-72.png" width="72" height="72" />
        <icon src="www/img/icon/ios7/Icon-72@2x.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="www/img/icon/ios7/Icon-small.png" width="29" height="29" />
        <icon src="www/img/icon/ios7/Icon-small@2x.png" width="58" height="58" />
     </platform>
</widget>
